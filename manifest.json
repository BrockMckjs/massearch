{
    "name": "Massearcher",
    "action": {
        "default_popup": "front.html"
    },
    "manifest_version": 3,
    "version": "2.1.1",
    "description": "Search multiple search engines",

    "chrome_url_overrides" : {
        "newtab": "front.html"
    },

    "permissions": [
        "tabs"
    ],

    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },


    "background": {
        "service_worker": "background.js"

    },

    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["search.js"]
    }]
}