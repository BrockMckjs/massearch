{
    "name": "Massearcher",
    "browser_action": {
        "default_popup": "front.html"
    },
    "manifest_version": 2,
    "version": "0.1",
    "description": "search multiple search engines",
    "permissions": [
        "activeTab",
        "tabs",
        "storage"

    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false

    },

    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["search.js", "background.js"]
    }],

    "content_security_policy": "script-src 'self' 'sha256-'"
}